{
	"name": "scradd",
	"version": "3.0.3",
	"private": true,
	"homepage": "https://discord.gg/scratch-coders-1141222489582735360",
	"type": "module",
	"main": "dist/index.js",
	"scripts": {
		"build": "rm -rf dist && tsc --project tsconfig.json",
		"dev": "npm run serve & npm run watch",
		"format": "prettier --write \"**\" --ignore-path .prettierignore --ignore-unknown --cache",
		"lint": "eslint .",
		"serve": "nodemon --env-file=.env --enable-source-maps . --ext js,json,html,css,png",
		"start": "node --env-file=.env --enable-source-maps .",
		"test": "npm run build && node --enable-source-maps --test",
		"watch": "tsc --project tsconfig.json --watch"
	},
	"browserslist": [
		"chrome 80",
		"edge 80",
		"opera 67",
		"ff 86"
	],
	"prettier": "@redguy12/prettier-config",
	"dependencies": {
		"axios": "^1.7.9",
		"discord.js": "14.16.0",
		"mongoose": "7.6.3",
		"papaparse": "^5.4.1",
		"strife.js": "4.0.1"
	},
	"devDependencies": {
		"@types/difflib": "<=0.2",
		"@types/eslint": "<=8.57",
		"@types/jsdom": "^21.1.7",
		"@types/mustache": "<=4.2",
		"@types/node": "<=20.10",
		"@types/papaparse": "<=5.4",
		"@typescript-eslint/eslint-plugin": "7.0.2",
		"@typescript-eslint/parser": "7.4.0",
		"eslint": "8.57.0",
		"eslint-plugin-unicorn": "51.0.1",
		"nodemon": "3.1.0",
		"prettier": "3.2.5",
		"typescript": "5.4.5"
	},
	"optionalDependencies": {
		"@railway/cli": "3.5.2"
	},
	"engines": {
		"node": ">=20.10.0",
		"npm": ">=8.0.0"
	}
}
